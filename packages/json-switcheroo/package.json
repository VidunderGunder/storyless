{
  "name": "@storyless/json-switcheroo",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "module": "index.ts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "description": "CLI Tool for overriding JSON files",
  "author": "Kristian Gunder Kram√•s <kristiankramas@outlook.com> (https://github.com/VidunderGunder)",
  "repository": {
    "type": "git",
    "url": "https://github.com/VidunderGunder/storyless.git",
    "directory": "packages/json-switcheroo"
  },
  "bin": {
    "@storyless/figma-to-svg": "./build/index.js"
  },
  "keywords": [
    "cli",
    "figma",
    "svg",
    "sync",
    "storyless",
    "ui"
  ],
  "files": [
    "build/**"
  ],
  "scripts": {
    "clean": "rm -rf node_modules .turbo build",
    "lint": "oxlint .",
    "build:dts": "tsc --project ./tsconfig.release.json",
    "build:js": "bun build ./index.ts --outdir ./build --target node && bun run post-build.ts",
    "build": "bun build:dts & bun build:js"
  },
  "type": "module",
  "dependencies": {
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "commander": "^11.1.0",
    "picocolors": "^1.0.0",
    "bun": "^1.0.13"
  },
  "devDependencies": {
    "bun-types": "^1.0.13",
    "typescript": "^5.2.2",
    "prettier": "3.1.0"
  }
}
