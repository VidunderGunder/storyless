{
  "name": "@storyless/react",
  "version": "0.0.7",
  "main": "index.tsx",
  "types": "index.tsx",
  "license": "MIT",
  "private": false,
  "type": "module",
  "author": "Kristian Gunder Kram√•s <kristiankramas@outlook.com> (https://github.com/VidunderGunder)",
  "repository": {
    "type": "git",
    "url": "https://github.com/VidunderGunder/storyless.git",
    "directory": "packages/react-storyless"
  },
  "keywords": [
    "storyless",
    "ui",
    "frontend",
    "devtool",
    "devtools",
    "react",
    "next",
    "nextjs"
  ],
  "files": [
    "build/**"
  ],
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.12"
  },
  "packageManager": "bun@1.0.12",
  "scripts": {
    "lint:oxlint": "oxlint .",
    "lint:eslint": "eslint .",
    "lint": "bun lint:eslint",
    "generate:component": "turbo gen react-component",
    "clean": "rm -rf build && rm -rf node_modules && rm -rf .turbo && rm -rf tsconfig.release.tsbuildinfo",
    "build:dts": "tsc --project ./tsconfig.release.json",
    "build:js": "bun build ./index.tsx --outdir ./build --external='react' --external='react-dom'",
    "build": "bun build:dts & bun build:js"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.16",
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "bun-types": "^1.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}